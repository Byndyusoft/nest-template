{
  "name": "ᐸAppᐳ",
  "version": "1.0.0",
  "private": true,
  "description": "ᐸApp descriptionᐳ",
  "homepage": "https://github.com/Byndyusoft/ᐸRepository nameᐳ#readme",
  "bugs": {
    "url": "https://github.com/Byndyusoft/ᐸRepository nameᐳ/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Byndyusoft/ᐸRepository nameᐳ.git"
  },
  "license": "Apache-2.0",
  "author": "Byndyusoft",
  "scripts": {
    "prebuild": "shx rm -rf ./dist",
    "build": "yarn run prebuild && yarn run build:src",
    "build:src": "nest build",
    "start": "nest start",
    "start:prod": "yarn node ./dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts",
      "!./src/infrastructure/**/*.ts",
      "!./src/main.ts",
      "!**/__tests__/**/*.ts",
      "!**/*Command.ts",
      "!**/*Dto.ts",
      "!**/*Entity.ts",
      "!**/*Mapper.ts",
      "!**/*Module.ts",
      "!**/*Query.ts",
      "!**/index.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov",
      "text",
      "@byndyusoft/sonar-coverage-reporter"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "resetMocks": true,
    "rootDir": "./",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "setupFiles": [
      "reflect-metadata"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "testResultsProcessor": "jest-sonar-reporter",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "@byndyusoft/class-validator-extended": "^1.0.0",
    "@byndyusoft/nest-opentracing": "^2.3.0",
    "@byndyusoft/nest-swagger": "^5.1.5-2",
    "@byndyusoft/pino-logger-factory": "^2.0.0",
    "@digikare/nestjs-prom": "^1.0.0",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.2.3",
    "@nestjs/core": "^8.2.3",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/terminus": "^8.0.3",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "helmet": "^4.6.0",
    "jaeger-client": "^3.19.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^2.3.1",
    "pg": "^8.7.1",
    "pino": "^7.5.1",
    "pino-http": "^6.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.2.0",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.41",
    "ᐸDtosᐳ": "1.0.0",
    "ᐸEntitiesᐳ": "1.0.0"
  },
  "devDependencies": {
    "@byndyusoft/sonar-coverage-reporter": "^1.0.0",
    "@byndyusoft/tsconfig": "^1.1.0",
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@types/express": "^4.17.13",
    "@types/jaeger-client": "^3.18.3",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.1",
    "@types/source-map-support": "^0.5.4",
    "@types/ts-nameof": "^4.2.1",
    "express": "^4.17.1",
    "jest": "^27.4.5",
    "jest-extended": "^1.2.0",
    "jest-mock-extended": "^2.0.4",
    "jest-sonar-reporter": "^2.0.0",
    "pino-pretty": "^7.3.0",
    "shx": "^0.3.3",
    "sonarqube-scanner": "^2.8.1",
    "ts-jest": "^27.1.1",
    "ts-nameof": "^5.0.0",
    "ts-transformer-keys": "^0.4.3",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1",
    "ᐸDtos testingᐳ": "1.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "jestSonar": {
    "reportPath": "./coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  }
}
