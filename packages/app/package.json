{
  "name": "ᐸAppᐳ",
  "version": "0.0.0-development",
  "private": true,
  "description": "ᐸApp descriptionᐳ",
  "homepage": "https://github.com/Byndyusoft/ᐸRepository nameᐳ#readme",
  "bugs": {
    "url": "https://github.com/Byndyusoft/ᐸRepository nameᐳ/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Byndyusoft/ᐸRepository nameᐳ.git"
  },
  "license": "Apache-2.0",
  "author": "Byndyusoft",
  "scripts": {
    "prebuild": "shx rm -rf ./dist",
    "build": "yarn run prebuild && yarn run build:src",
    "build:src": "nest build",
    "start": "nest start --exec \"yarn node -r dotenv/config\"",
    "start:prod": "yarn node ./dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts",
      "!./src/infrastructure/**/*.ts",
      "!./src/main.ts",
      "!**/*Command.ts",
      "!**/*Dto.ts",
      "!**/*Entity.ts",
      "!**/*Mapper.ts",
      "!**/*Module.ts",
      "!**/*Query.ts",
      "!**/index.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov",
      "text",
      "@byndyusoft/sonar-coverage-reporter"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "reporters": [
      "default",
      [
        "@casualbot/jest-sonar-reporter",
        {
          "relativePaths": true,
          "outputDirectory": "coverage",
          "outputName": "test-report.xml"
        }
      ]
    ],
    "resetMocks": true,
    "roots": [
      "<rootDir>/test"
    ],
    "setupFiles": [
      "reflect-metadata"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "@byndyusoft/class-validator-extended": "1.2.0",
    "@byndyusoft/nest-opentracing": "2.3.2",
    "@byndyusoft/nest-pino": "3.4.0-1",
    "@byndyusoft/nest-swagger": "7.1.9-1",
    "@byndyusoft/pino-logger-factory": "3.0.2-rc.1",
    "@digikare/nestjs-prom": "1.0.0",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.4",
    "@nestjs/core": "10.2.4",
    "@nestjs/platform-express": "10.2.4",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "helmet": "7.0.0",
    "jaeger-client": "3.19.0",
    "lodash": "4.17.21",
    "pg": "8.11.3",
    "pino": "8.15.0",
    "pino-http": "8.4.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "0.5.21",
    "tslib": "2.6.2",
    "typeorm": "0.3.17",
    "ᐸDtosᐳ": "workspace:*",
    "ᐸEntitiesᐳ": "workspace:*"
  },
  "devDependencies": {
    "@byndyusoft/sonar-coverage-reporter": "1.0.0",
    "@byndyusoft/tsconfig": "2.0.0",
    "@casualbot/jest-sonar-reporter": "2.2.7",
    "@nestjs/cli": "10.1.17",
    "@nestjs/schematics": "10.0.2",
    "@types/express": "4.17.17",
    "@types/jaeger-client": "3.18.4",
    "@types/jest": "29.5.4",
    "@types/lodash": "4.14.197",
    "@types/node": "20.5.7",
    "@types/pg": "8.10.2",
    "@types/source-map-support": "0.5.7",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "jest": "29.6.4",
    "jest-extended": "4.0.1",
    "jest-mock-extended": "3.0.5",
    "pino-pretty": "10.2.0",
    "shx": "0.3.4",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2",
    "ᐸDtos testingᐳ": "workspace:*"
  },
  "engines": {
    "node": ">=16"
  }
}
