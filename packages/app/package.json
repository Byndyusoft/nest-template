{
  "name": "ᐸAppᐳ",
  "version": "1.0.0",
  "private": true,
  "description": "ᐸApp descriptionᐳ",
  "homepage": "https://github.com/Byndyusoft/ᐸRepository nameᐳ#readme",
  "bugs": {
    "url": "https://github.com/Byndyusoft/ᐸRepository nameᐳ/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Byndyusoft/ᐸRepository nameᐳ.git"
  },
  "license": "Apache-2.0",
  "author": "Byndyusoft",
  "scripts": {
    "prebuild": "shx rm -rf ./dist",
    "build": "yarn run prebuild && nest build",
    "postinstall": "(ts-patch install) || true",
    "start": "nest start",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts",
      "!./src/infrastructure/**/*.ts",
      "!./src/main.ts",
      "!**/__tests__/**/*.ts",
      "!**/*Command.ts",
      "!**/*Dto.ts",
      "!**/*Entity.ts",
      "!**/*Module.ts",
      "!**/*Query.ts",
      "!**/index.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "resetMocks": true,
    "rootDir": "./",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "setupFiles": [
      "reflect-metadata"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "testResultsProcessor": "jest-sonar-reporter",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "@byndyusoft/casc": "^2.0.0",
    "@byndyusoft/class-validator-extended": "^1.0.0",
    "@byndyusoft/nest-opentracing": "https://github.com/Byndyusoft/nest-dynamic-module/releases/download/init-snapshot/byndyusoft-nest-opentracing-2.3.0-feature-dynamic-http-client-2.tgz",
    "@byndyusoft/pino-logger-factory": "^1.0.1",
    "@digikare/nestjs-prom": "^1.0.0",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "https://github.com/KillWolfVlad/swagger/releases/download/support-external-dtos-snapshot/nestjs-swagger-5.0.9-feature-support-external-dtos-2.tgz",
    "@nestjs/terminus": "^8.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "execa": "^5.1.1",
    "helmet": "^4.6.0",
    "jaeger-client": "^3.18.1",
    "nestjs-pino": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "source-map-support": "^0.5.20",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.3.1",
    "ᐸDtosᐳ": "1.0.0"
  },
  "devDependencies": {
    "@byndyusoft/tsconfig": "^1.1.0",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@types/express": "^4.17.13",
    "@types/jaeger-client": "^3.18.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.6",
    "@types/source-map-support": "^0.5.4",
    "@types/ts-nameof": "^4.2.1",
    "express": "^4.17.1",
    "jest": "^27.2.0",
    "jest-extended": "^0.11.5",
    "jest-mock-extended": "^2.0.2-beta2",
    "jest-sonar-reporter": "^2.0.0",
    "shx": "^0.3.3",
    "sonarqube-scanner": "^2.8.1",
    "ts-jest": "^27.0.5",
    "ts-nameof": "^5.0.0",
    "ts-patch": "^1.4.4",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1",
    "ᐸDtos testingᐳ": "1.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "jestSonar": {
    "reportPath": "./coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  }
}
